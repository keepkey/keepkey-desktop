{
  "name": "keepkey-sdk-server",
  "version": "0.1.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn build:spec && tsoa routes && tsc --build",
