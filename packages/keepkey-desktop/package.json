{
    "name": "keepkey-desktop",
    "version": "3.0.12",
    "author": {
        "name": "KeepKey",
        "email": "support@keepkey.com"
    },
    "description": "An all-in-one suite for using your KeepKey hardware wallet.",
    "repository": {
        "type": "git",
        "url": "https://github.com/keepkey/keepkey-desktop"
    },
    "homepage": "undefined",
    "private": true,
    "scripts": {
        "build": "ts-node --skipProject --swc ./scripts/build.ts",
        "release": "yarn run -T build && rimraf build/metafile.json build/app/metafile.json && DEBUG=electron-builder electron-builder",
        "electron-builder": "DEBUG=electron-builder electron-builder",
        "dev": "ts-node --skipProject --swc ./scripts/dev.ts",
        "clean": "rimraf build dist",
        "lint": "eslint -c .eslintrc src --ext .ts,.tsx",
        "lint:fix": "yarn lint --fix"
    },
    "main": "build/main.js",
    "build": {
        "appId": "com.keepkey.desktop",
        "productName": "KeepKey Desktop",
        "afterSign": "./scripts/afterSign.js",
        "generateUpdatesFilesForAllChannels": true,
        "publish": "github",
        "files": [
            "build/**/*"
        ],
        "protocols": [
            {
                "name": "Keepkey Desktop",
                "schemes": [
                    "keepkey"
                ]
            }
        ],
        "asar": true,
        "extends": null,
        "mac": {
            "hardenedRuntime": true,
            "category": "public.app-category.finance",
            "icon": "icon.png",
            "entitlements": "entitlements.mac.plist",
            "entitlementsInherit": "entitlements.mac.plist",
            "target": [
                "dmg"
            ]
        },
        "nsis": {
            "differentialPackage": false
        },
        "win": {
            "icon": "icon.png",
            "target": [
                "nsis"
            ]
        },
        "linux": {
            "icon": "icon.png",
            "target": [
                "deb",
                "AppImage"
            ]
        }
    },
    "dependencies": {
        "@electron/notarize": "^2.3.0",
        "@ethersproject/contracts": "^5.7.0",
        "@keepkey/device-protocol": "^7.10.2",
        "@keepkey/hdwallet-core": "^1.53.9",
        "@keepkey/hdwallet-keepkey-nodehid": "^1.53.9",
        "@keepkey/hdwallet-keepkey-nodewebusb": "^1.53.9",
        "@sentry/electron": "^4.1.0",
        "@shapeshiftoss/chain-adapters": "^11.2.2",
        "@shapeshiftoss/errors": "^1.1.3",
        "@shapeshiftoss/logger": "^1.1.3",
        "@tsoa/runtime": "^4.1.3",
        "auto-launch": "^5.0.5",
        "bigint-buffer": "^1.1.5",
        "body-parser": "^1.20.1",
        "bs58": "^5.0.0",
        "bs58check": "^3.0.1",
        "bufferutil": "^4.0.7",
        "builder-util": "^25.0.0-alpha.6",
        "comlink": "^4.3.1",
        "comlink-electron-endpoint": "^1.0.3",
        "common-utils": "workspace:*",
        "cors": "^2.8.5",
        "create-hash": "^1.2.0",
        "dotenv": "^16.0.3",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.8",
        "electron-unhandled": "^4.0.1",
        "electron-updater": "^6.1.8",
        "esbuild": "^0.15.14",
        "esbuild-plugin-resolve": "^1.0.3",
        "ethers": "^6.8.0",
        "express": "^4.18.2",
        "fsevents": "^2.3.2",
        "fswin": "^3.22.106",
        "hidefile": "^3.0.0",
        "jimp": "^0.16.2",
        "jsqr": "^1.4.0",
        "keccak": "^3.0.2",
        "keepkey-desktop-app": "workspace:*",
        "keepkey-sdk-server": "workspace:*",
        "lodash": "^4.17.21",
        "long": "^5.2.3",
        "nedb-promises": "^6.2.1",
        "node-fetch": "^3.3.0",
        "node-hid": "^2.1.2",
        "promise.any": "^2.0.5",
        "protobufjs": "^7.2.6",
        "secp256k1": "^4.0.3",
        "semver": "^7.3.8",
        "source-map-support": "^0.5.21",
        "swagger-ui-dist": "^4.15.5",
        "swagger-ui-express": "^4.6.0",
        "tiny-secp256k1": "^1.1.6",
        "usb": "^2.5.2",
        "utf-8-validate": "^5.0.10",
        "uuid": "^9.0.0",
        "wait-promise": "^0.4.1",
        "web3": "^4.0.1"
    },
    "devDependencies": {
        "@keepkey/common-esbuild-bits": "workspace:*",
        "@shapeshiftoss/eslint-plugin-logger": "^1.1.0",
        "@shapeshiftoss/hdwallet-keepkey": "^1.52.11",
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.19",
        "@tsoa/cli": "^4.1.3",
        "@types/auto-launch": "^5.0.2",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.14",
        "@types/hidefile": "^3.0.0",
        "@types/lodash": "^4.14.190",
        "@types/node": "^20",
        "@types/pnpapi": "^0.0.2",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.0",
        "@types/wait-promise": "^0.4.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "concurrently": "^7.6.0",
        "electron": "22.0.0",
        "electron-builder": "24.0.0-alpha.5",
        "electron-notarize-dmg": "^1.0.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "tsoa": "^4.1.3",
        "typescript": "^4.7.0"
    }
}
